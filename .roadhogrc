{
  "entry": "./src/index.js",
  "autoprefixer": {
    "browsers": [
      ">1%",
      "ie>=8"
    ]
  },
  "hash": true,
  "theme": "./src/theme.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    }
  }
}